{
  "request": {
    "method": "GET",
    "urlPattern": "/AssureIDService/Document/[\\S]+"
  },
  "response": {
    "status": 200,
    "transformers": [ "get-result-response-definition-transformer", "response-template"],
    "transformerParameters": {
      "ognlExpression": "{{jsonPath(state context=request.pathSegments.[2] property='image') '$.front' }}"
    }
  }
}
