{
  "request": {
    "method": "POST",
    "urlPattern": "/AssureIDService/Document/[^/]+/Image.side=(front|back|0|1)&light=0",
    "queryParameters": {
      "side": {
        "matches": "^(back|1)$"
      }
    }
  },
  "response": {
    "status": 200
  },
  "serveEventListeners": [
    {
      "name": "recordState",
      "parameters": {
        "context": "{{request.pathSegments.[2]}}",
        "state": {
          "instanceId": "{{request.pathSegments.[2]}}",
          "back_image": "{{request.body}}",
          "front_image": "{{ state context=request.pathSegments.[2] property='front_image' default='' }}"
        }
      }
    }
  ]
}
